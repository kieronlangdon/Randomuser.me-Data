user
$base_url   = http://api.randomuser.me/portraits/
$version    = 0.1
$nat        = IE

gender      = {gender}

name        = {array, 3}
title       = {if, {!gender}, ==, male, mr, {~2QZP74k}}
first       = {if, {!gender}, ==, male, {~2eTRbLz}, {~HosftIT}}
last        = {~gbKJWTP}

location    = {array, 4}
street      = {random, numeric, 1000, 9999} {~hd23qvW}
city        = {~OSeLsPB}
state       = {~d5AeCoL}
zip         = {random, numeric, 10000, 99999}}

email       = {!first}.{!last}{random, numeric, 10, 99}@example.com
username    = {~rgjs5Ec}{~agqg7Ff}{random, numeric, 1, 999}

password    = {~3yolDfd}
salt        = {random, special, 1, 8}
$pass_salt  = {!password}{!salt}
md5         = {hash, md5, {$pass_salt}}
sha1        = {hash, sha1, {$pass_salt}}
sha256      = {hash, sha256, {$pass_salt}}

registered  = {random, numeric, 915148700, {timestamp}}
dob         = {random, numeric, 0, 504921500}

phone       = 07{random, special, 2, 2}-{random, special, 2, 3}-{random, special, 2, 3}
cell        = 0{if, {gender}, ==, male, 1, 2}{random, special, 2, 3}-{random, special, 2, 3}-{random, special, 2, 3}

// Nino calculation
$nino_1     = {~FeITwGh}
$nino_2     = {~pfx4HcX}
NINO        = {upper, {$nino_1}{$nino_2} {random, special, 2, 2} {random, special, 2, 2} {random, special, 2, 2} {random, special, 5, 1}}

picture     = {array, 3}
$pic_id     = {if, {!gender}, ==, male, {random, numeric, 0, 99}, {random, numeric, 0, 96}}
large       = {if, {!gender}, ==, male, {$base_url}men/{$pic_id}.jpg, {$base_url}women/{$pic_id}.jpg}
medium      = {if, {!gender}, ==, male, {$base_url}med/men/{$pic_id}.jpg, {$base_url}med/women/{$pic_id}.jpg}
thumbnail   = {if, {!gender}, ==, male, {$base_url}thumb/men/{$pic_id}.jpg, {$base_url}thumb/women/{$pic_id}.jpg}

version     = {$version}

nationality = {$nat}

